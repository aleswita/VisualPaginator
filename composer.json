{
  "name": "aleswita/visualpaginator",
  "description": "VisualPaginator for Nette Framework.",
  "type": "library",
  "keywords": [
    "nette",
    "visual paginator",
    "paginator"
  ],
  "license": ["MIT"],
  "homepage": "https://github.com/aleswita/VisualPaginator",
  "authors": [
    {
      "name": "Ales Wita"
    }
  ],
  "require": {
    "php": "^7.4|8.0",
    "nette/application": "^3.1",
    "nette/utils": "^3.2",
    "nette/http": "^3.1",
    "nette/forms": "^3.0",
    "latte/latte": "^2.9"
  },
  "require-dev": {
    "nette/di": "^3.0",
    "nette/robot-loader": "^3.3",
    "nette/routing": "^3.0",
    "nette/bootstrap": "^3.1",
    "tracy/tracy": "^2.8",
    "nette/tester": "^2.3",
    "ninjify/nunjuck": "^0.3",
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "AlesWita\\Components\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "tests/App",
      "Tests\\": "tests/Tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}
